{
    "name": "cuongnd88/delivery-channels",
    "description": "Laravel Notification Delivery Channels",
    "authors": [
        {
            "name": "Ngo Dinh Cuong",
            "email": "dinhcuongngo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/notifications": "^6.0|^7.0",
        "illuminate/routing": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "twilio/sdk": "^6.0"
    },
    "require-dev": {
        "illuminate/config": "^6.0|^7.0",
        "mockery/mockery": "~1.3.1",
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Cuongnd88\\DeliveryChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cuongnd88\\DeliveryChannel\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuongnd88\\DeliveryChannel\\DeliveryChannelServiceProvider"
            ]
        }
    }
}
